{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "ignorePatterns": [
    "src/generated/**/*",
    "src/generated/prisma/**/*",
    "node_modules/**/*",
    ".next/**/*",
    "out/**/*"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
} 